{
  "name": "Python Dev Container",
  "image": "mcr.microsoft.com/vscode/devcontainers/python:3.11",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker",
        "esbenp.prettier-vscode",
        "aaron-bond.better-comments",
        "alefragnani.bookmarks",
        "amazonwebservices.aws-toolkit-vscode",
        "bierner.markdown-preview-github-styles",
        "bodil.prettier-toml",
        "charliermarsh.ruff",
        "csstools.postcss",
        "davidanson.vscode-markdownlint",
        "donjayamanne.githistory",
        "donjayamanne.python-environment-manager",
        "donjayamanne.python-extension-pack",
        "doublebot.doublebot",
        "eamodio.gitlens",
        "ecmel.vscode-html-css",
        "esbenp.prettier-vscode",
        "github.copilot",
        "github.copilot-chat",
        "github.vscode-pull-request-github",
        "grapecity.gc-excelviewer",
        "huizhou.githd",
        "hyunkyunmoon.gzipdecompressor",
        "ibm.output-colorizer",
        "inferrinizzard.prettier-sql-vscode",
        "kevinrose.vsc-python-indent",
        "mechatroner.rainbow-csv",
        "medo64.code-point",
        "mhutchie.git-graph",
        "mikestead.dotenv",
        "mohsen1.prettify-json",
        "ms-azuretools.vscode-docker",
        "ms-edgedevtools.vscode-edge-devtools",
        "ms-python.black-formatter",
        "ms-python.debugpy",
        "ms-python.flake8",
        "ms-python.isort",
        "ms-python.mypy-type-checker",
        "ms-python.pylint",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter-renderers",
        "ms-toolsai.vscode-jupyter-cell-tags",
        "ms-toolsai.vscode-jupyter-powertoys",
        "ms-toolsai.vscode-jupyter-slideshow",
        "ms-vscode-remote.remote-containers",
        "ms-vscode-remote.remote-ssh",
        "ms-vscode-remote.remote-ssh-edit",
        "ms-vscode-remote.vscode-remote-extensionpack",
        "ms-vscode.live-server",
        "ms-vscode.remote-explorer",
        "ms-vscode.remote-server",
        "njpwerner.autodocstring",
        "openai.openai-chatgpt-adhoc",
        "phplasma.csv-to-table",
        "postman.postman-for-vscode",
        "redhat.vscode-yaml",
        "rogalmic.bash-debug",
        "rvest.vs-code-prettier-eslint",
        "shd101wyy.markdown-preview-enhanced",
        "tamasfe.even-better-toml",
        "tintinweb.graphviz-interactive-preview",
        "usernamehw.errorlens",
        "visualstudioexptteam.intellicode-api-usage-examples",
        "visualstudioexptteam.vscodeintellicode",
        "wholroyd.jinja",
        "yzhang.markdown-all-in-one"
      ],
      "settings": {
        "python.formatting.provider": "black",
        "python.linting.enabled": true,
        "python.linting.pylintEnabled": true,
        "python.linting.mypyEnabled": true
      }
    }
  },
  "postCreateCommand": "curl -sSL https://install.python-poetry.org | python3 - && export PATH=\"$HOME/.local/bin:$PATH\" && poetry install",
  "remoteUser": "vscode",
  "mounts": [
    "source=${localWorkspaceFolder}/.env,target=/workspace/.env,type=bind",
    "source=${localEnv:HOME}/.ssh,target=/home/vscode/.ssh,type=bind"
  ],
  "forwardPorts": [],
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind",
  "workspaceFolder": "/workspace"
}
